{
  "name": "@porkbellypro/crm",
  "private": true,
  "engines": {
    "node": "^14.17.5",
    "npm": "^7.20.6"
  },
  "scripts": {
    "build": "npm run build-server && npm run build-web",
    "build-server": "npm run -w server build",
    "build-web": "npx web-build -o dist -t \"PorkBelly CRM\"",
    "lint": "npm run --ws lint",
    "start": "npx server -d dist"
  },
  "workspaces": [
    "server",
    "web"
  ]
}